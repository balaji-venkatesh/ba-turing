{
    "name": "ba-turing",
    "productName": "ba-turing",
    "version": "0.2.0-beta",
    "description": "ba-turing is a better turing IDE, based on Electron JS and Node JS.",
    "main": "src/index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make"
    },
    "keywords": [
        "Electron",
        "Turing",
        "IDE",
        "editor",
        "language",
        "learning"
    ],
    "author": "Balaji Venkatesh <balaji.venkatesh@hotmail.com>",
    "license": "BA",
    "config": {
        "forge": {
            "make_targets": {
                "win32": [
                    "squirrel"
                ],
                "darwin": [
                    "dmg",
                    "zip"
                ],
                "linux": [
                    "deb",
                    "rpm"
                ]
            },
            "electronPackagerConfig": {},
            "electronWinstallerConfig": {
                "name": "ba_turing"
            },
            "electronInstallerDebian": {},
            "electronInstallerRedhat": {},
            "github_repository": {
                "owner": "balaji-venkatesh",
                "name": "ba-turing"
            },
            "windowsStoreConfig": {
                "packageName": "",
                "name": "baturing"
            }
        }
    },
    "dependencies": {
        "electron-compile": "^6.4.4",
        "monaco-loader": "^0.8.2"
    },
    "devDependencies": {
        "electron-forge": "^3.0.5",
        "electron-prebuilt-compile": "1.4.15"
    }
}
